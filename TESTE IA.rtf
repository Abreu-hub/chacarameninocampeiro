  <script>\par
    /***** CONFIG - substitua aqui *****/\par
    const API_KEY = "AIzaSyBb_kpmqNlrFfi3r2wzNtFJnOAwGjbenms";         // \f2\u8592?\f3  \f0 coloque sua chave aqui\par
    const CHANNEL_ID = "UC1qDH9or0_PEtt7hVGBBkeA";   // \f2\u8592?\f3  \f0 coloque o ID do canal aqui\par
    /*************************************/\par
\par
    // containers\par
    const videosContainer = document.getElementById('videos');\par
    const livesContainer  = document.getElementById('lives');\par
    const shortsContainer = document.getElementById('shorts');\par
    const videosCountEl   = document.getElementById('videosCount');\par
    const livesCountEl    = document.getElementById('livesCount');\par
    const shortsCountEl   = document.getElementById('shortsCount');\par
\par
    const modal = document.getElementById('videoModal');\par
    const iframe = document.getElementById('playerIframe');\par
    const closeModalBtn = document.getElementById('closeModal');\par
\par
    // Parse ISO8601 duration (e.g. PT1H2M3S) -> seconds\par
    function parseISO8601Duration(iso) \{\par
      if (!iso) return 0;\par
      const match = iso.match(/PT(?:(\\d+)H)?(?:(\\d+)M)?(?:(\\d+)S)?/);\par
      if (!match) return 0;\par
      const hours = parseInt(match[1]||0,10);\par
      const minutes = parseInt(match[2]||0,10);\par
      const seconds = parseInt(match[3]||0,10);\par
      return hours*3600 + minutes*60 + seconds;\par
    \}\par
\par
    function formatDate(iso) \{\par
      try \{\par
        const d = new Date(iso);\par
        return d.toLocaleDateString('pt-BR', \{ day:'2-digit', month:'short', year:'numeric' \});\par
      \} catch(e)\{ return iso; \}\par
    \}\par
\par
    // get uploads playlist id for channel\par
    async function getUploadsPlaylistId() \{\par
      const url = `https://www.googleapis.com/youtube/v3/channels?part=contentDetails&id=$\{CHANNEL_ID\}&key=$\{API_KEY\}`;\par
      const res = await fetch(url);\par
      const json = await res.json();\par
      if (json.error) throw new Error(json.error.message || 'Erro ao buscar canal');\par
      const items = json.items || [];\par
      if (!items.length) throw new Error('Canal n\'e3o encontrado (verifique CHANNEL_ID)');\par
      return items[0].contentDetails.relatedPlaylists.uploads;\par
    \}\par
\par
    // fetch all items from a playlist (paginated)\par
    async function fetchAllPlaylistVideoIds(playlistId) \{\par
      let videoIds = [];\par
      let pageToken = '';\par
      do \{\par
        const url = `https://www.googleapis.com/youtube/v3/playlistItems?part=contentDetails&playlistId=$\{playlistId\}&maxResults=50$\{pageToken?`&pageToken=$\{pageToken\}`:''\}&key=$\{API_KEY\}`;\par
        const res = await fetch(url);\par
        const json = await res.json();\par
        if (json.error) throw new Error(json.error.message || 'Erro playlistItems');\par
        (json.items || []).forEach(it => \{\par
          if (it.contentDetails && it.contentDetails.videoId) videoIds.push(it.contentDetails.videoId);\par
        \});\par
        pageToken = json.nextPageToken || '';\par
      \} while (pageToken);\par
      return videoIds;\par
    \}\par
\par
    // fetch video details in batches of up to 50\par
    async function fetchVideoDetailsBatch(ids) \{\par
      const batches = [];\par
      for (let i=0;i<ids.length;i+=50) \{\par
        batches.push(ids.slice(i,i+50));\par
      \}\par
      const results = [];\par
      for (const batch of batches) \{\par
        const url = `https://www.googleapis.com/youtube/v3/videos?part=snippet,contentDetails,liveStreamingDetails&id=$\{batch.join(',')\}&maxResults=50&key=$\{API_KEY\}`;\par
        const res = await fetch(url);\par
        const json = await res.json();\par
        if (json.error) throw new Error(json.error.message || 'Erro videos.list');\par
        (json.items || []).forEach(it => results.push(it));\par
      \}\par
      return results;\par
    \}\par
\par
    // create card element (thumbnail + info)\par
    function createCard(video) \{\par
      const id = video.id;\par
      const title = video.snippet.title;\par
      const thumb = (video.snippet.thumbnails && (video.snippet.thumbnails.medium || video.snippet.thumbnails.high || video.snippet.thumbnails.default))?.url\par
                    || `https://img.youtube.com/vi/$\{id\}/hqdefault.jpg`;\par
      const published = video.snippet.publishedAt ? formatDate(video.snippet.publishedAt) : '';\par
      const card = document.createElement('div');\par
      card.className = 'video-card';\par
      card.innerHTML = `\par
        <img class="thumb" src="$\{thumb\}" alt="$\{escapeHtml(title)\}" loading="lazy">\par
        <div class="video-info">\par
          <div class="title">$\{escapeHtml(title)\}</div>\par
          <div class="meta">$\{published\}</div>\par
        </div>\par
      `;\par
      card.dataset.videoId = id;\par
      card.addEventListener('click', () => openModalWithVideo(id));\par
      return card;\par
    \}\par
\par
    function escapeHtml(text) \{\par
      return text.replaceAll('&','&amp;').replaceAll('<','&lt;').replaceAll('>','&gt;');\par
    \}\par
\par
    function openModalWithVideo(videoId) \{\par
      iframe.src = `https://www.youtube.com/embed/$\{videoId\}?autoplay=1&rel=0`;\par
      modal.classList.add('open');\par
      modal.setAttribute('aria-hidden','false');\par
    \}\par
\par
    function closeModal() \{\par
      iframe.src = '';\par
      modal.classList.remove('open');\par
      modal.setAttribute('aria-hidden','true');\par
    \}\par
\par
    closeModalBtn.addEventListener('click', closeModal);\par
    modal.addEventListener('click', (e) => \{\par
      if (e.target === modal) closeModal();\par
    \});\par
    document.addEventListener('keydown', (e) => \{ if (e.key === 'Escape') closeModal(); \});\par
\par
    // main\par
    async function loadAndClassifyAll() \{\par
      try \{\par
        // clear containers and show loaders\par
        videosContainer.innerHTML = `<div class="small-note loader">Carregando v\'eddeos...</div>`;\par
        livesContainer.innerHTML  = `<div class="small-note loader">Carregando lives...</div>`;\par
        shortsContainer.innerHTML = `<div class="small-note loader">Carregando shorts...</div>`;\par
\par
        const uploadsPlaylistId = await getUploadsPlaylistId();\par
        const allVideoIds = await fetchAllPlaylistVideoIds(uploadsPlaylistId);\par
\par
        if (!allVideoIds.length) \{\par
          videosContainer.innerHTML = '<div class="small-note">Nenhum v\'eddeo encontrado.</div>';\par
          livesContainer.innerHTML  = '<div class="small-note">Nenhuma live encontrada.</div>';\par
          shortsContainer.innerHTML = '<div class="small-note">Nenhum short encontrado.</div>';\par
          return;\par
        \}\par
\par
        // fetch details\par
        const details = await fetchVideoDetailsBatch(allVideoIds);\par
\par
        // classify\par
        const videosList = [];\par
        const livesList  = [];\par
        const shortsList = [];\par
\par
        details.forEach(v => \{\par
          const durationSec = parseISO8601Duration(v.contentDetails?.duration || '');\par
          const isShort = durationSec > 0 && durationSec <= 60;\par
          const hasLiveDetails = !!v.liveStreamingDetails;\par
          const liveBroadcastFlag = (v.snippet && v.snippet.liveBroadcastContent && v.snippet.liveBroadcastContent !== 'none');\par
\par
          // classify as live if liveStreamingDetails present OR flag set (live/upcoming) OR if actualStartTime exists\par
          const isLive = hasLiveDetails || liveBroadcastFlag;\par
\par
          if (isLive) \{\par
            livesList.push(v);\par
          \} else if (isShort) \{\par
            shortsList.push(v);\par
          \} else \{\par
            videosList.push(v);\par
          \}\par
        \});\par
\par
        // sort each by publishedAt desc\par
        const sortByDateDesc = arr => arr.sort((a,b)=> new Date(b.snippet.publishedAt) - new Date(a.snippet.publishedAt));\par
        sortByDateDesc(videosList); sortByDateDesc(livesList); sortByDateDesc(shortsList);\par
\par
        // render\par
        renderList(videosContainer, videosList);\par
        renderList(livesContainer, livesList);\par
        renderList(shortsContainer, shortsList);\par
\par
        videosCountEl.textContent = videosList.length;\par
        livesCountEl.textContent  = livesList.length;\par
        shortsCountEl.textContent = shortsList.length;\par
\par
      \} catch (err) \{\par
        console.error(err);\par
        const msg = `Erro ao carregar v\'eddeos: $\{err.message || err\}`;\par
        videosContainer.innerHTML = `<div class="small-note">$\{msg\}</div>`;\par
        livesContainer.innerHTML  = `<div class="small-note">$\{msg\}</div>`;\par
        shortsContainer.innerHTML = `<div class="small-note">$\{msg\}</div>`;\par
      \}\par
    \}\par
\par
    function renderList(container, items) \{\par
      container.innerHTML = '';\par
      if (!items.length) \{\par
        container.innerHTML = '<div class="small-note">Nenhum item encontrado.</div>';\par
        return;\par
      \}\par
      items.forEach(item => \{\par
        const card = createCard(item);\par
        container.appendChild(card);\par
      \});\par
    \}\par
\par
    // start on load\par
    window.addEventListener('load', () => \{\par
      if (!API_KEY || API_KEY === 'YOUR_API_KEY') \{\par
        const msg = '\f4\u9888?\u-497?\f3  \f0 Insira sua API_KEY e CHANNEL_ID no topo do arquivo antes de carregar os v\'eddeos.';\par
        videosContainer.innerHTML = `<div class="small-note">$\{msg\}</div>`;\par
        livesContainer.innerHTML  = `<div class="small-note">$\{msg\}</div>`;\par
        shortsContainer.innerHTML = `<div class="small-note">$\{msg\}</div>`;\par
        return;\par
      \}\par
      loadAndClassifyAll();\par
    \});\par
\par
\par
  </script>\par
